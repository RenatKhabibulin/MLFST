[build-system]
requires = ["setuptools>=61.0"]
build-backend = "setuptools.build_meta"

[project]
name = "FS_tutorial_app"
version = "0.1.0"
description = "Interactive platform for learning ML in fire safety applications"
readme = "README.md"
requires-python = ">=3.7"
dependencies = [
    "streamlit>=1.0.0",
    "pandas",
    "numpy",
    "matplotlib",
    "plotly",
    "scikit-learn",
    "sqlalchemy",
    "psycopg2-binary",
    "scipy",
    "seaborn",
    "anthropic",
    "openai",
]