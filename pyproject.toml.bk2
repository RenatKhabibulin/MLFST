[build-system]
requires = ["setuptools>=61.0", "wheel"]
build-backend = "setuptools.build_meta"

[project]
name = "mlfiresafetytutor"
version = "0.1.0"
authors = [
    {name = "Author", email = "author@example.com"},
]
description = "ML FireSafetyTutor - Interactive platform for learning ML in fire safety applications"
readme = "readme.txt"
requires-python = ">=3.10"
classifiers = [
    "Programming Language :: Python :: 3",
    "License :: OSI Approved :: MIT License",
    "Operating System :: OS Independent",
]
dependencies = [
    "streamlit>=1.31.0",
    "pandas>=2.0.0",
    "numpy>=1.24.0",
    "matplotlib>=3.7.0",
    "plotly>=5.14.0",
    "scikit-learn>=1.3.0",
    "sqlalchemy>=2.0.0",
    "psycopg2-binary>=2.9.5",
    "scipy>=1.10.0", 
    "seaborn>=0.12.0",
    "anthropic>=0.3.0",
    "openai>=0.27.0",
]

[project.urls]
"Homepage" = "https://github.com/example/mlfiresafetytutor"
"Bug Tracker" = "https://github.com/example/mlfiresafetytutor/issues"

[tool.setuptools]
py-modules = []